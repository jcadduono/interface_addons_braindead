<Bindings>
	<Binding name="Toggle Targets +" header="BRAINDEAD" category="ADDONS">
		Braindead_ToggleTargetMode()
	</Binding>
	<Binding name="Toggle Targets -" category="ADDONS">
		Braindead_ToggleTargetModeReverse()
	</Binding>
	<Binding name="Set Targets to 1" category="ADDONS">
		Braindead_SetTargetMode(1)
	</Binding>
	<Binding name="Set Targets to 2" category="ADDONS">
		Braindead_SetTargetMode(2)
	</Binding>
	<Binding name="Set Targets to 3" category="ADDONS">
		Braindead_SetTargetMode(3)
	</Binding>
	<Binding name="Set Targets to 4+" category="ADDONS">
		Braindead_SetTargetMode(4)
	</Binding>
</Bindings>
